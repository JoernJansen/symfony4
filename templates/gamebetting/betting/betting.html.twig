            {{ form_row(form.firstTeamResult) }}
            {{ form_row(form.secondTeamResult) }}
            <input type="hidden" name="user_betting[gameId]" value="{{ gameId }}">
        {{ form_end(form) }}

    {% endfor %}

{% endblock %}